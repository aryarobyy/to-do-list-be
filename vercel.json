{
  "devCommand": "ts-node src/main.ts",
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "http://localhost:4000/socket.io/$1" },
    { "src": "/user(.*)",      "dest": "http://localhost:4000/user$1" },
    { "src": "/note(.*)",      "dest": "http://localhost:4000/note$1" },
    { "src": "/(.*)",          "dest": "/$1" }
  ]
}